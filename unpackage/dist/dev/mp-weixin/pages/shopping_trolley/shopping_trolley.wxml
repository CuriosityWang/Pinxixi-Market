<view><view class="big"><block wx:if="{{!flag}}"><view class="headCss">购物车空空如也，请先<label data-event-opts="{{[['tap',[['gotoChoise',['$event']]]]]}}" class="choise _span" bindtap="__e">选购</label></view></block><block wx:else><view><block wx:for="{{myData}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="cartitem"><view class="top"><view class="twoleft"><checkbox-group data-event-opts="{{[['change',[['selected',['$0'],[[['myData','',index]]]]]]]}}" bindchange="__e"><checkbox style="transform:scale(0.7);" color="orange" checked="{{item.flag}}"></checkbox></checkbox-group><label class="_span">{{"商品："+item.name}}</label></view><view class="del"><uni-icons class="del" vue-id="{{'daedbb00-1-'+index}}" color="orange" type="trash" size="18" data-event-opts="{{[['^tap',[['del',['$0',index],[[['myData','',index]]]]]]]}}" bind:tap="__e" bind:__l="__l"></uni-icons></view></view><view class="center"><view class="mainContain"><view><image src="{{'http://image.nssnail.com/'+item.goods.fileList[0].relativePath}}"></image></view><view class="main"><view class="descript">{{item.goods.description+''}}<view class="twoBtn"><view style="color:red;">{{"￥"+item.myPrice}}</view><view class="buybtn"><uni-icons vue-id="{{'daedbb00-2-'+index}}" color="orange" type="minus" size="15" data-event-opts="{{[['^tap',[['reduce',['$0'],[[['myData','',index]]]]]]]}}" bind:tap="__e" bind:__l="__l"></uni-icons><label class="myqtys _span">{{item.qty}}</label><uni-icons vue-id="{{'daedbb00-3-'+index}}" color="orange" type="plus" size="15" data-event-opts="{{[['^tap',[['add',['$0'],[[['myData','',index]]]]]]]}}" bind:tap="__e" bind:__l="__l"></uni-icons></view></view></view></view></view></view></view></block><view class="three"><view class="choose"><checkbox-group data-event-opts="{{[['change',[['selectedall']]]]}}" bindchange="__e"><checkbox style="transform:scale(0.7);" checked="{{allchecked}}" color="orange"></checkbox>全选</checkbox-group></view><view class="two"><view class="total">{{'合计：'+totalPrice+''}}</view><button class="allNumber" disabled="{{disabled}}" data-event-opts="{{[['tap',[['submit',['$event']]]]]}}" bindtap="__e">{{'结算:（ '+totalNum+' )'}}</button></view></view></view></block></view></view>